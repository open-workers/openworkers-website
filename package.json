    "@angular/platform-browser-dynamic": "15.2.6",
    "@angular/platform-server": "15.2.6",
    "@angular/router": "15.2.6",
    "@nguniversal/express-engine": "^15.2.0",
    "express": "^4.15.2",
    "rehype-highlight": "^6.0.0",
    "rehype-stringify": "^9.0.3",
    "remark-breaks": "^3.0.2",
