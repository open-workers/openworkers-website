{
  "name": "app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve --host 127.0.0.1 --port 4300",
    "dev:prod": "ng serve --host 127.0.0.1 --port 4300 --configuration production",
    "start": "ng serve",
    "build": "ng build --configuration production",
